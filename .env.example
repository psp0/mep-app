# Database Configuration
JDBC_DATABASE_URL=jdbc:mysql://mysql:3306/mepdb?useSSL=false&allowPublicKeyRetrieval=true&serverTimezone=Asia/Seoul&characterEncoding=UTF-8&connectionCollation=utf8mb4_unicode_ci
JDBC_DATABASE_USERNAME=mepuser
JDBC_DATABASE_PASSWORD=your_password_here

# MySQL Configuration
MYSQL_DATABASE=mepdb
MYSQL_USER=mepuser
MYSQL_PASSWORD=your_password_here
MYSQL_ROOT_PASSWORD=your_root_password_here

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here
JWT_ACCESS_TOKEN_VALIDITY_SECONDS=3600
JWT_REFRESH_TOKEN_VALIDITY_SECONDS=604800

# Domain Configuration
DOMAIN_NAME=example.com

# Swagger Configuration
SWAGGER_SERVER_URL=https://example.com
SWAGGER_SERVER_PORT=443

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080,https://example.com
